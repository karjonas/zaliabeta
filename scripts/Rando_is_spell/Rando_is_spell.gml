/// @description  Rando_is_spell(spell)
function Rando_is_spell() {


	return argument[0]==STR_PROTECT 
	   ||  argument[0]==STR_JUMP 
	   ||  argument[0]==STR_HEAL 
	   ||  argument[0]==STR_FAIRY 
	   ||  argument[0]==STR_FIRE 
	   ||  argument[0]==STR_REFLECT 
	   ||  argument[0]==STR_ENIGMA 
	   ||  argument[0]==STR_THUNDER 
	   ||  argument[0]==STR_SUMMON;
	//







}

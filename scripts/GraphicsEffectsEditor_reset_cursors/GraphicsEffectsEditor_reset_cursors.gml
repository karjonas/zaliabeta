/// @description  GraphicsEffectsEditor_reset_cursors(Main_cursor)
function GraphicsEffectsEditor_reset_cursors(argument0) {


	if (argument0>=0) Main_cursor = argument0;
	Brightness_cursor = 0;
	Saturation_cursor = 0;
	Scanlines_cursor  = 0;
	Bloom_cursor      = 0;
	Blur_cursor       = 0;







}
